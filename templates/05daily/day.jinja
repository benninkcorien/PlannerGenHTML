{% extends "./base.html" %} 
{% block content %}
  {% for date in allthedates %}
    <div class="leftside">
      <div id="{{ date.strftime('%Y-%m-%d') }}"></div>
      <h2>{{ date.strftime('%Y-%m-%d') }} - {{ date.strftime('%A') }} - {{ date.strftime('%B') }}</h2>
      <div class="dailyschedule">
        {% for _ in range(24) %}
          <hr />
        {% endfor %}
      </div>
      <a href="#{{ (date - timedelta(days=1)).strftime('%Y-%m-%d') }}">Previous</a>
      <a href="#{{ (date + timedelta(days=1)).strftime('%Y-%m-%d') }}">Next</a>
    </div>
    <div class="pagebreak"></div>
  {% endfor %}
{% endblock content %}
